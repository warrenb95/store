<%= link_to "Back to all users", store_users_path %>

<h1><%= @user.full_name %></h1>
<p><%= @user.email_address %></p>

<div>
  <%= link_to "Edit user", edit_store_user_path(@user) %>
  <%= button_to "Delete user",
  store_user_path(@user),
  method: :delete,
  data: {
    turbo_confirm: "Are you sure?",
  } %>
</div>
